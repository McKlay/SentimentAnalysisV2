{
    "build": {
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "builder": "NIXPACKS",
      "buildCommand": "pip install -r requirements.txt",
      "watchPatterns": ["**/*.py", "requirements.txt"],
      "root": "backend/app"
    },
    "deploy": {
      "startCommand": "uvicorn main:app --host 0.0.0.0 --port 8000"
    }
  }
  